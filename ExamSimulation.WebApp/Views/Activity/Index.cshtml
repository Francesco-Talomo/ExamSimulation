@model IEnumerable<ExamSimulation.Classes.ActivityForGuest>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CountPartecipant)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Date)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CountPartecipant)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Date)
            </td>
        </tr>
    }
</table>